{
    "scripts": {
        "start": "node --inspect=8080 -r ts-node/register ./src/server.ts",
        "start:watch": "nodemon",
        "build": "tsc",
        "initdb": "ts-node src/utils/initdb"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.16",
        "@types/pg": "^7.14.8",
        "eslint": "^7.16.0",
        "eslint-plugin-import": "^2.22.1",
        "fs-extra": "^9.0.1",
        "nodemon": "^2.0.6",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "pg": "^8.5.1",
        "pg-promise": "^10.8.7"
    }
}
